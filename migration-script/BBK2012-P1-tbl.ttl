eozol:BBK2012.P1.132 eozol:country "NA" ;
    eozol:grade "NA" ;
    eozol:olympiad "NA" ;
    eozol:problem_number "NA" ;
    eozol:problemid "BBK2012.P1.132" ;
    eozol:text """Ar $d(i)$ apzīmēsim skaitļa $i$ naturālo dalītāju skaitu.
Pierādiet, ka jebkuram naturālam skaitlim $n$ izpildās vienādība
$$d(1)+d(2)+\\ldots+d(n)= \\left\\lfloor \\frac{n}{1} \\right\\rfloor +
\\left\\lfloor \\frac{n}{2} \\right\\rfloor + \\ldots +
\\left\\lfloor \\frac{n}{n} \\right\\rfloor.$$"""@lv ;
    eozol:year "NA" .

