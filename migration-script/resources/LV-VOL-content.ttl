    eliozo:problemBookSection "NA" ;
    eliozo:problemGrade 12 ;
    eliozo:problemID "LV.VOL.2003.12.1" ;
    eliozo:problemText """Vai eksistē
 tādi naturāli skaitļi $x$, $y$ un $z$,
kas lielāki par $1$, ka $x! \\cdot y! = z!$?
 tādi naturāli skaitļi $a, b, c, d, e$, kas lielāki par $1$, ka
$a! \\cdot b! \\cdot c! \\cdot d! = e!$?"""@lv ;
    eliozo:problemYear "2003" ;
    eliozo:problem_number 1 .

eliozo:LV.VOL.2003.12.4 a eliozo:Problem ;
    eliozo:country "LV" ;
