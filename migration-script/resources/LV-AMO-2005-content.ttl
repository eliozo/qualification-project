eliozo:LV.AMO.2005.11.5 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:domain "" ;
    eliozo:event "LV.AMO.2005" ;
    eliozo:olympiad "LV.AMO" ;
