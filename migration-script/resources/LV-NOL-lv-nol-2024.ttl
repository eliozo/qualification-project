    eliozo:problemSolution eliozo:SOLN.LV.NOL.2024.10.2 ;
    eliozo:problemText "Doti reāli skaiții $x$ un $y$, kuriem $x y^{3}+1=x+y^{3}$. Pierādīt, ka $y x^{3}+1=y+x^{3}$."@lv ;
    eliozo:problemYear "2024" ;
    eliozo:problem_number 2 .

eliozo:LV.NOL.2024.10.3 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:olympiad eliozo:LV.NOL ;
    eliozo:olympiadCode "NOL" ;
