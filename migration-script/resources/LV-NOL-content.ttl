eliozo:LV.NOL.2000.7.5 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:event "LV.NOL.2000" ;
    eliozo:genre "MakingMoves" ;
    eliozo:olympiad "LV.NOL" ;
