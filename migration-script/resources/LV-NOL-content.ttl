    eliozo:seeAlso "LV.NOL.2019.8.4" ;
    eliozo:strategy "Invariant" ;
    eliozo:suffix "LV.NOL.2000.7" ;
    eliozo:suggestedGrade 7 ;
    eliozo:topic eliozo:InvariantAlgebraic .

