        eliozo:InductionWithLargerStep,
        eliozo:InductionWithStepOne,
        eliozo:StrentheningInductiveHypothesis,
        eliozo:StrongInduction,
        eliozo:TwoDirectionalInduction ;
    skos:prefLabel "MathematicalInduction" .

