eliozo:LV.AMO.2013.6.1 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:domain "Alg" ;
    eliozo:event "LV.AMO.2013" ;
    eliozo:olympiad "LV.AMO" ;
