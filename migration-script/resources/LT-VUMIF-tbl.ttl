    eliozo:problemGrade 9 ;
    eliozo:problemID "LT.VUMIF.2019.9.1" ;
    eliozo:problemText """Atrodiet visus veselu skaitļu trijniekus $(x,y,z)$,
kas apmierina vienādojumu sistēmu
$$
\\left \\begin{array}{l}
x^3 + y^3 = z^3 + 1,\\
y^2 - x^2 = x+y,\\
2x^3 - 6x = z^3 - 4x^2.
\\end{array} \\right.$$"""@lv ;
    eliozo:problemYear "2019" ;
    eliozo:problem_number "1" .

