eliozo:LV.TST.1994.12.2 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:event "LV.TST.1994" ;
    eliozo:olympiad "LV.TST" ;
    eliozo:olympiadCode "TST" ;
    eliozo:olympiadType "TeamSelection" ;
    eliozo:problemGrade 12 ;
    eliozo:problemID "LV.TST.1994.12.2" ;
    eliozo:problemText """Vai var atrast tādus $2^{1994}$
dažādus naturālu skaitļu pārus
$\\left( a_i,b_i \\right)$, 
ka vienlaikus izpildās šādas prasības:

1. ${\\displaystyle \\frac{1}{a_1b_1} + \\frac{1}{a_2b_2} + \\cdots + \\frac{1}{a_{2^{1994}}b_{2^{1994}}} = 1}$;   
2. ${\\displaystyle \\left( a_1 + a_2 + \\cdots + a_{2^{1994}}\\right) + \\left( b_1 + b_2 + \\cdots + b_{2^{1994}}\\right) = 3^{1995}}$."""@lv ;
    eliozo:problemTextHtml """<p>Vai var atrast tādus $2^{1994}$
dažādus naturālu skaitļu pārus
$\\left( a_i,b_i \\right)$, 
ka vienlaikus izpildās šādas prasības:</p>
<ol>
<li>${\\displaystyle \\frac{1}{a_1b_1} + \\frac{1}{a_2b_2} + \\cdots + \\frac{1}{a_{2^{1994}}b_{2^{1994}}} = 1}$;   </li>
<li>${\\displaystyle \\left( a_1 + a_2 + \\cdots + a_{2^{1994}}\\right) + \\left( b_1 + b_2 + \\cdots + b_{2^{1994}}\\right) = 3^{1995}}$.</li>
</ol>"""@lv ;
    eliozo:problemTimeID "1994" ;
    eliozo:problemYear 1994 ;
    eliozo:problem_number 2 ;
    eliozo:questionType "ProveDisprove" ;
    eliozo:suffix "LV.TST.1994.12" ;
    eliozo:suggestedGrade 12 .

