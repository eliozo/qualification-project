eliozo:LV.AMO.2008.10.4 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:domain "Comb" ;
    eliozo:event "LV.AMO.2008" ;
    eliozo:olympiad "LV.AMO" ;
