    eliozo:olympiad eliozo:LT.LJKMO ;
    eliozo:olympiadCode "LJKMO" ;
    eliozo:problemGrade 7 ;
    eliozo:problemID "LT.LJKMO.2002.7_8.1" ;
    eliozo:problemText """Ja no skaitļu virknes $2$, $4$, $6$, $8$, $10$, $12$,
$14$ un $16$ izsvītrotu $4$, $8$, $10$, $14$ un $16$,
tad atlikušo skaitļu $2$, $6$ un $12$ reizinājums būtu
$144 = 12 \\cdot 12$, tātad būtu pilns kvadrāts.
Kāds ir mazākais skaitļu skaits, ko jāizsvītro no sākotnējās virknes, lai
atlikušo skaitļu reizinājums būtu pilns kvadrāts?"""@lv ;
    eliozo:problemYear "2002" ;
    eliozo:problem_number "1" .

