eozol:LV.AO.2013.5.4 eozol:video [ a eozol:Video ;
            eozol:videoBookmarks [ a rdf:Seq ;
                    rdf:_1 [ eozol:bmtext "Uzdevuma saprašana: Tetromino, t.sk. spēle \"tetris\". Pentomino. Pentomino \"L\" figūra un tās spoguļattēls." ;
                            eozol:tstamp 21 ] ;
                    rdf:_10 [ eozol:bmtext "Atskats: Nebija prasīts vai var salikt taisnstūri 4x10. Nebija prasīts, vai šāds salikums ir viens vienīgs." ;
                            eozol:tstamp 452 ] ;
                    rdf:_2 [ eozol:bmtext "Uzdevuma saprašana: Teselācijas jeb flīzēšanas uzdevumi – salikt laukumu ar noteikta veida figūriņām bez tukšumiem un pārklāšanās. (Kopīga mala ir saskaršanās, nevis pārklāšanās.)" ;
                            eozol:tstamp 57 ] ;
                    rdf:_3 [ eozol:bmtext "Uzdevuma saprašana: Kas ir pagriešana un apgriešana otrādi." ;
                            eozol:tstamp 85 ] ;
                    rdf:_4 [ eozol:bmtext "Risinājums: Taisnstūris ar laukumu 40 rūtiņas izsakāms 4 veidos, no kuriem 2 veidi acīmredzot neder. Metode: Dalīšana reizinātājos. Izslēgšanas metode." ;
                            eozol:tstamp 144 ] ;
                    rdf:_5 [ eozol:bmtext "Risinājums: Cik var būt \"L\" figūriņu? No 1 līdz 7. Metode: Izmantojam nosacījumus." ;
                            eozol:tstamp 236 ] ;
                    rdf:_6 [ eozol:bmtext "Risinājums: \"L\" figūriņu nevar būt 1,3,5,7. Nevar būt arī 2 vai 6. Metode: Pāru un nepāru skaitļi. Dalāmība ar 4." ;
                            eozol:tstamp 267 ] ;
                    rdf:_7 [ eozol:bmtext "Risinājums: Atlikušās 20 rūtiņas jāaizpilda ar 5 \"T\" figūriņām. Metode: Ģeometrijas jautājuma izteikšana aritmētiski." ;
                            eozol:tstamp 318 ] ;
                    rdf:_8 [ eozol:bmtext "Risinājums: Aplūkojam kā 2 \"L\" figūriņas var saskarties un kā aizpildīt atlikušo taisnstūri. Metode: Heiristika (sākt aizpildīt ar lielākajām figūrām). mēģinājumu un kļūdu metode." ;
                            eozol:tstamp 330 ] ;
                    rdf:_9 [ eozol:bmtext "Atskats: Konstrukcijas uzdevumiem pats piemērs kalpo par pierādījumu." ;
                            eozol:tstamp 400 ] ] ;
            eozol:videoTitle "40.AMO, 5.klases 4.uzdevums" ;
            eozol:youtubeID "aPtiwtx9Jkg" ] .

eozol:LV.AO.2013.5.5 eozol:video [ a eozol:Video ;
            eozol:videoBookmarks [ a rdf:Seq ;
                    rdf:_1 [ eozol:bmtext "Uzdevuma saprašana: Kas ir kubs? Kuba skaldnes, šķautnes, virsotnes." ;
                            eozol:tstamp 24 ] ;
                    rdf:_2 [ eozol:bmtext "Uzdevuma saprašana: Kā katru skaldni sadalīt 4 kvadrātos. Cik pavisam daļu?" ;
                            eozol:tstamp 110 ] ;
                    rdf:_3 [ eozol:bmtext "Uzdevuma saprašana: Kāda ir krāsošana 3 krāsās (nav vajadzīgas krāsas - var apzīmēt ar burtiem); un kādi kvadrāti ir ar kopīgu malu?" ;
                            eozol:tstamp 149 ] ;
                    rdf:_4 [ eozol:bmtext "Risinājums: Pamatojums, ka ar divām krāsām nepietiek. Metode: Dirihlē princips." ;
                            eozol:tstamp 225 ] ;
                    rdf:_5 [ eozol:bmtext "Risinājums: Ērtāka kuba krāsošanas diagramma. Metode: Nepārtraukts pārveidojums (Telpiskas figūras attēlojums plaknē)." ;
                            eozol:tstamp 340 ] ;
                    rdf:_6 [ eozol:bmtext "Risinājums: Izveidojam krāsojumu. Piemērs: Piemēra konstruēšana." ;
                            eozol:tstamp 495 ] ;
                    rdf:_7 [ eozol:bmtext "Atskats: Ko nozīmē \"Atbildi pamatot!\"? Konstrukcijas uzdevumos ir vai nu jāpiedāvā konstrukcija vai jāpamato, ka to nav iespējams veikt (t.i. neviens un nekad nespēs to izdarīt ar 2 krāsām – nevis tikai \"mēs mēģinājām un mums nesanāca\")." ;
                            eozol:tstamp 644 ] ;
                    rdf:_8 [ eozol:bmtext "Atskats: Ir pazīstama t.s. \"četru krāsu problēma\" – ka ikvienas kartes izkrāsošanai pietiek ar 4 krāsām (vienalga kādu izmēru gabaliņiem). Šoreiz karte ir īpatnēja – un tādēļ nav pārsteidzoši, ka pietiek ar mazāku krāsu skaitu." ;
                            eozol:tstamp 735 ] ;
                    rdf:_9 [ eozol:bmtext "Atskats: Atrisinājumā jāmin (a) neiespējamība; (b) konstruēts piemērs." ;
                            eozol:tstamp 820 ] ] ;
            eozol:videoTitle "40.AMO, 5.klases 5.uzdevums" ;
            eozol:youtubeID "YQAQ3e3cfqo" ] .

eozol:LV.AO.2013.6.1 eozol:video [ a eozol:Video ;
            eozol:videoBookmarks [ a rdf:Seq ;
                    rdf:_1 [ eozol:bmtext "Uzdevuma saprašana: \"Jebkurus divus\" skaitļus var izvēlēties ļoti daudzos veidos. Būtu liktenīga kļūda aplūkot tikai vienu vai divus piemērus." ;
                            eozol:tstamp 33 ] ;
                    rdf:_2 [ eozol:bmtext "Uzdevuma saprašana: Nav pateikts, kur ieraksta jauno skaitli, bet secība šeit nav svarīga." ;
                            eozol:tstamp 86 ] ;
                    rdf:_3 [ eozol:bmtext "Uzdevuma saprašana: Neatkarīgi no darbību secības tieši pēc 9 gājieniem viss būs beidzies." ;
                            eozol:tstamp 119 ] ;
                    rdf:_4 [ eozol:bmtext "Risinājums: Kas notiktu, ja nodzēšto skaitļu vietā rakstītu vienkārši \"a+b\". Metode: Pāreja uz vienkāršāku uzdevumu." ;
                            eozol:tstamp 154 ] ;
                    rdf:_5 [ eozol:bmtext "Risinājums: Kā ātri saskaitīt 1+2+...+N. Metode: Saskaitīšana ar pārgrupēšanu." ;
                            eozol:tstamp 228 ] ;
                    rdf:_6 [ eozol:bmtext "Risinājums: Visu uzrakstīto skaitļu summa mainās, bet mainās prognozējami - pieaug par 2 pēc katras dzēšanas. Metode: Invariantu izmantošana." ;
                            eozol:tstamp 315 ] ;
                    rdf:_7 [ eozol:bmtext "Risinājums: Pēc 9 soļiem būs visu skaitļu summa plus 18. Atrodam visu skaitļu summu un pieskaitām 18. Metode: Saskaitīšanas pārtapšana par reizināšanu." ;
                            eozol:tstamp 335 ] ;
                    rdf:_8 [ eozol:bmtext "Atskats: Var pārbaudīt 1-2 gadījumiem." ;
                            eozol:tstamp 370 ] ;
                    rdf:_9 [ eozol:bmtext "Atskats: Sekas no operācijas (a,b)-> a+b+2 komutativitātes un asociativitātes. (Komutativitāte nodrošina to, ka nav svarīga secība, kā izvēlas a un b. Asociativitāte nodrošina to, ka nav svarīgi, kā zīmē koku.)" ;
                            eozol:tstamp 438 ] ] ;
            eozol:videoTitle "40.AMO, 6.klases 1.uzdevums" ;
            eozol:youtubeID "VAqkROCPuKk" ] .

