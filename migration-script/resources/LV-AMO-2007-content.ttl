eliozo:LV.AMO.2007.11.5 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:domain "" ;
    eliozo:event "LV.AMO.2007" ;
    eliozo:olympiad "LV.AMO" ;
