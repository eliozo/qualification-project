    eozol:year "2003" .

eozol:LV.AO.2003.12.2 eozol:country "LV" ;
    eozol:grade 12 ;
    eozol:olympiad "AO" ;
    eozol:problem_number "2" ;
    eozol:problemid "LV.AO.2003.12.2" ;
    eozol:text """Vai eksistē tāds vesels pozitīvs skaitlis $n$,
ka skaitlim $n^2$ ir tikpat daudz naturālu dalītāju,
kas dod atlikumu $1$, dalot ar $3$,
cik naturālu dalītāju, kas dod atlikumu $2$, dalot ar $3$?"""@lv ;
    eozol:year "2003" .

eozol:LV.AO.2003.7.3 eozol:country "LV" ;
    eozol:grade 7 ;
    eozol:olympiad "AO" ;
