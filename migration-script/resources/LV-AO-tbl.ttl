eliozo:LV.AO.2003.7.5 a eliozo:Problem ;
    eliozo:country "LV" ;
    eliozo:hasSkill eliozo:comb.full,
        eliozo:misc.invar.parity,
        eliozo:seq.arithm.summation ;
    eliozo:olympiad eliozo:LV.AO ;
    eliozo:olympiadCode "AO" ;
    eliozo:problemGrade 7 ;
    eliozo:problemID "LV.AO.2003.7.5" ;
    eliozo:problemText """Uz tāfeles pa reizei uzrakstīti visi naturālie skaitļi no $1$ līdz $n$ ieskaitot.
Ar vienu gājienu var izvēlēties divus uz tāfeles uzrakstītus skaitļus
(apzīmēsim tos ar $a$ un $b$), nodzēst tos un to vietā uzrakstīt $\\left| a^2-b^2 \\right|$.
Pēc $n-1$ gājiena uz tāfeles paliek viens skaitlis.
Vai tas var būt $0$, ja  $n=8$,  $n=9$?"""@lv ;
    eliozo:problemYear "2003" ;
    eliozo:problem_number "5" .

