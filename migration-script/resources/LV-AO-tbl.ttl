    eozol:problemid "LV.AO.2010.7.2" ;
    eozol:text """Caur trijstūra $ABC$ virsotni $A$ novilktā taisne $t$ sadala trijstūri divos
vienādos trijstūros.
Vai var gadīties, ka $AB>AC$?"""@lv ;
    eozol:year "2010" .

eozol:LV.AO.2010.7.3 eozol:country "LV" ;
    eozol:grade 7 ;
