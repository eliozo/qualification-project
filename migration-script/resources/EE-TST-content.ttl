eliozo:EE.TST.2003.12.3 a eliozo:Problem ;
    eliozo:country "EE" ;
    eliozo:event "EE.TST.2003" ;
    eliozo:olympiad "EE.TST" ;
    eliozo:olympiadCode "TST" ;
    eliozo:olympiadType "TeamSelection" ;
