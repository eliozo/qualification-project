    eliozo:problemYear 2017 ;
    eliozo:problem_number 11 ;
    eliozo:suffix "EE.TST.2017.12" ;
    eliozo:suggestedGrade 12 .

eliozo:EE.TST.2017.12.7 a eliozo:Problem ;
